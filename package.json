{
  "name": "truffle-init-webpack",
  "version": "0.0.1",
  "description": "Frontend example using truffle v3 + added React",
  "scripts": {
    "lint": "eslint ./",
    "build": "webpack -p",
    "testrpc:docker:build": "docker build -t decent/testrpc docker/testrpc",
    "testrpc:start": "docker run -p 8545:8545 -t decent/testrpc testrpc -m \"hello build tongue rack parade express shine salute glare rate spice stock\" -a 10",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --content-base build/  --watch --hot --inline --config  webpack.config.js",
    "truffle:migrate": "truffle migrate",
    "truffle:start": "node truffle",
    "bridge:start": "cd ethereum-bridge && node bridge.js -H localhost:8545 -a 9 --dev",
    "explorer:start": "cd explorer && npm start"
  },
  "engines": {
    "node": ">=6.9.1 <6.9.1"
  },
  "author": "Douglas von Kohorn",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.2",
    "truffle": "^3.2.5",
    "truffle-contract": "^1.1.6",
    "url-loader": "^0.5.8",
    "web3": "^0.18.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "moment": "^2.18.1",
    "moment-countdown": "0.0.3"
  }
}
